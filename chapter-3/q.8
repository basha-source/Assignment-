def binary_search(arr, search_key):
 left, right = 0, len(arr) - 1
 comparisons = 0
 
 while left <= right:
 mid = (left + right) // 2
 comparisons += 1
 if arr[mid] == search_key:
 return mid, comparisons
 elif arr[mid] < search_key:
 left = mid + 1
 else:
 right = mid - 1
 
 return -1, comparisons
arr1 = [5, 10, 15, 20, 25, 30, 35, 40, 45]
search_key1 = 20
index1, comparisons1 = binary_search(arr1, search_key1)
print(f"Test Case 1 - Index: {index1}, Comparisons: {comparisons1}")
