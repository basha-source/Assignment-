def find_min_max(arr):
    if not arr:
        return None, None
    
    min_val = arr[0]
    max_val = arr[0]
    
    for num in arr:
        if num < min_val:
            min_val = num
        if num > max_val:
            max_val = num
    
    return min_val, max_val

# Test cases
arrays = [
    [5, 7, 3, 4, 9, 12, 6, 2],
    [1, 3, 5, 7, 9, 11, 13, 15, 17],
    [22, 34, 35, 36, 43, 67, 12, 13, 15, 17]
]

for arr in arrays:
    min_val, max_val = find_min_max(arr)
    print(f"Input: N={len(arr)}, a[] = {arr}")
    print(f"Output: Min = {min_val}, Max = {max_val}")
    print()
